{
  "name": "my-vue-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "preview:dev": "vite --mode development",
    "preview:prd": "vite --mode production",
    "build:dev": "vite build --mode development",
    "build:prd": "vite build --mode production",
    "bulid:ts:dev": "tsc --noEmit --skipLibCheck vite build --mode development",
    "bulid:ts:prd": "tsc --noEmit --skipLibCheck vite build --mode production",
    "preview": "vite preview",
    "ext:eslint": "eslint --ext .vue,.js,.ts,.jsx,.tsx src",
    "fix:eslint": "eslint --fix --ext .vue,.js,.ts,.jsx,.tsx src",
    "fix:prettier": "prettier --write  \"src/**/*.{js,jsx,ts,tsx,vue,css,less,html,json,md}\""
  },
  "dependencies": {
    "@ant-design/icons-vue": "^7.0.1",
    "@types/store": "^2.0.5",
    "ant-design-vue": "4.x",
    "axios": "^1.6.8",
    "less": "^4.2.0",
    "pinia": "^2.1.7",
    "store": "^2.0.12",
    "vue": "^3.4.21",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/node": "^20.11.28",
    "@typescript-eslint/eslint-plugin": "^7.2.0",
    "@typescript-eslint/parser": "^7.2.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-vue": "^9.23.0",
    "prettier": "^3.2.5",
    "typescript": "^5.2.2",
    "vite": "^5.1.6",
    "vite-plugin-svg-icons": "^2.0.1",
    "vue-tsc": "^1.8.27"
  }
}
